<configuration>
	<appSettings>
		<add key="webpages:Version" value="3.0.0.0" />
		<add key="webpages:Enabled" value="false" />
		<add key="ClientValidationEnabled" value="true" />
		<add key="UnobtrusiveJavaScriptEnabled" value="true" />

		<add key="AAO_API_URL" value="http://127.0.0.1/api/" />
		<add key="LogfilePath" value="C:\Logs\AAO.Apps.BCSCSelfAssessment\AAO.Apps.BCSCSelfAssessment.log" />
		<!--<add key="ApiUrl" value="http://qas.aao.org/api/" />-->
		<add key="ApiUrl" value="https://qas.aao.org/api/" />
		<add key="To" value="bcscsapfeedback@aao.org" />
		<add key="Subject" value="BCSC Self-Assessment Program feedback on Question ID:" />
		<add key="host" value="mailrelay.aao.org" />
		<add key="port" value="25" />

		<add key="IndividualQuestionTime" value="120" />
		<add key="SimulatedQuestionCount" value="260" />
		<add key="QuickQuestionCount" value="15" />
		<add key="SimulatedQuestionTime" value="18000" />
		<add key="ShareResultPDFPath" value="C:\Logs\PDFResponseData\" />

		<add key="STORE_URL" value="https://store.aao.org/basic-and-clinical-science-course-self-assessment-program.html" />
		<add key="SSO_URL" value="#{AAO.Site.URL}/aao/Login?returnurl=" />
		<add key="RETURN_SITE_URL" value="#{Parent.Site.URL}" />
		<add key="SSO_API_URL" value="#{AAO.Site.URL}/AAO.Controllers/api/BCSC/GetCustomerInformation" />

		<!-- <add key="SSOlogout_URL" value="https://secure.aao.org/aao/logout"/> -->
		<add key="SSOlogout_URL1" value="#{AAO.Site.URL}/aao/logout?relayto="/>

		<add key="ShareResultSubject" value="Review BCSC Exam Results from " />
		<add key="SharedExamSubject" value="New Shared Exam in BCSC Self-Assessment Program" />
		<add key="BCSCSiteUrl" value="https://www.aao.org" />
		<add key="BCSCEmail" value="bcscsapfeedback@aao.org" />
		<add key="CustomerEmail" value="customer_service@aao.org" />
		<add key="IsDebugMode" value="N" />
		<add key="BCSCBooksDirectory" value="C:\AAO\sync\BCSC Book\" />
		<add key="CookieExpiry" value="1" />
		<add key="ImagesToBeShown" value="4,10,11,5,8,13,3,6,12,1,2,7,9" />
		<add key="RecordPerSheet" value="300000"/>
		<add key="RecordPerAPICall" value="5000"/>
		<add key="EXAM_HISTORY_LINK" value="#{Parent.Site.URL}/ExamHistory/Index"/>
	</appSettings>
	<system.web>
		<customErrors mode="Off" />
		<compilation debug="false" targetFramework="4.6.1" /> <!-- debug="true"-->
		<httpRuntime targetFramework="4.6.1" />
		<httpModules>
			<add name="ApplicationInsightsWebTracking" type="Microsoft.ApplicationInsights.Web.ApplicationInsightsHttpModule, Microsoft.AI.Web" />
		</httpModules>
		<!--Put Value in Minutes only-->
		<sessionState mode="InProc" timeout="20" cookieless="false"></sessionState>
		<authentication mode="Forms">
			<!-- <forms loginUrl="~/Home/Index" defaultUrl="~/Home/Index" name="AAOMASTER_SUBDOMAIN" timeout="2880"></forms>-->
			<!-- <forms loginUrl="~/Login/LogOffafterTimeOut" defaultUrl="~/Login/LogOffafterTimeOut" name="AAOMASTER_SUBDOMAIN" timeout="20"></forms>-->
			<forms loginUrl="~/Home/Index" defaultUrl="~/Home/Index" name="AAOMASTER_SUBDOMAIN" ></forms>
		</authentication>		
		<roleManager enabled="true" defaultProvider="MyRoleProvider" cacheRolesInCookie="true">
			<providers>
				<clear />
				<add name="MyRoleProvider" type="AAO.Apps.BCSCSelfAssessment.CustomeHelper.AAORoleProvider" />
			</providers>
		</roleManager>
	</system.web>
	<runtime>
		<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
			<dependentAssembly>
				<assemblyIdentity name="Newtonsoft.Json" culture="neutral" publicKeyToken="30ad4fe6b2a6aeed" />
