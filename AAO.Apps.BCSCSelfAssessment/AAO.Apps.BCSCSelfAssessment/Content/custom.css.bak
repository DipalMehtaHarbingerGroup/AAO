            padding-top: 0px;
            margin-top: 0;
            margin-bottom: 0;
            display: inline-block !important;
            width: 20px !important;
            text-align: left !important;
        }
    }

    /*Header Search*/


.header-search {
    border-radius: 4px;
    width: 177px;
    padding: 0;
    border: 1px solid #ccc;
    border-radius: 5px;
    box-shadow: inset 0 1px 1px rgb(0 0 0 / 8%);
    margin: 1rem;
    float: right;
}

    .header-search .input-group input {
        border: 0;
        border-right: 0;
        border-top-right-radius: 0;
        border-bottom-right-radius: 0;
        display: block;
        height: 30px;
        line-height: 28px;
    }

    .header-search .input-group-btn {
        position: relative;
        font-size: 0;
        white-space: nowrap;
    }

    .header-search #searchButton {
        background: #fff;
        border: 0;
    }


    .header-search .input-group-btn:last-child > .btn, .header-search .input-group-btn:last-child > .btn-group {
        margin-left: -1px;
    }

    .header-search #searchButton .icn-search {
        background: url(img/header_Search_icon.png) no-repeat;
        background-size: 16px auto;
        height: 17px;
        width: 16px;
        display: block;
        margin-top: 3px;
    }

