    -webkit-box-shadow: 0px 9px 23px -5px rgba(133,131,133,0.14);
    -moz-box-shadow: 0px 9px 23px -5px rgba(133,131,133,0.14);
    box-shadow: 0px 9px 23px -5px rgba(133,131,133,0.14);
}

.navbar {
    border-radius: 0;
}

.navbar-default .navbar-nav > .active > a, .navbar-default .navbar-nav > .active > a:hover, .navbar-default .navbar-nav > .active > a:focus {
    background-color: transparent;
    color: #f69339 !important;
    display: block;
    /* border-bottom: 1px solid #f68d2e; */
    /* padding-bottom:8px; */
}

.navbar-nav > li > a {
    padding-top: 15px;
    padding-bottom: 8px;
}

.navbar-default .navbar-nav > .active > a {
    border-bottom: 1px solid #f68d2e;
    bottom: 0;
}

.main-menu li a:hover i, nav li.active a i {
    color: #f69339 !important;
}

.main-menu li a {
    color: #000 !important;
    transition: color 0.5s, background-color 0.3s;
    outline: none;
}

    .main-menu li a:hover {
        color: #f69339 !important;
        display: block;
        /* border-bottom: 1px solid #f68d2e; */
        /* padding-bottom:8px; */
    }

.main-menu li a {
    position: relative;
    display: block;
    margin: 0;
    letter-spacing: 1px;
    font-size: 12px;
    line-height: 16px;
    font-weight: 900;
    text-transform: capitalize;
    transition: color 0.1s,background-color 0.1s,padding 0.2s ease-in;
    color: #000;
    padding:10px
}

    .main-menu li a::before {
        content: "";
        position: absolute;
        width: 100%;
        height: 1px;
        bottom: 0;
        left: 0;
        background-color: #f68d2e;
        visibility: hidden;
        -webkit-transform: scaleX(0);
        transform: scaleX(0);
        -webkit-transition: all 0.3s ease-in-out 0s;
        transition: all 0.3s ease-in-out 0s;
    }

    .main-menu li a:hover:before {
        visibility: visible;
        -webkit-transform: scaleX(1);
        transform: scaleX(1);
    }

    .main-menu li a:active::before {
        background-color: #000;
    }

    .main-menu li a:hover::before, a:focus::before {
        transform-origin: left top;
        transform: scale(1, 1);
        bottom: 0;
    }


.main-menu li:hover, nav li a:hover i {
    color: #f69339 !important;
}

.main-menu li i {
    color: #8679a3;
    transition: color 0.5s, background-color 0.3s;
    height: 20px;
}

.menu-icons {
    display: block;
    color: #3987d1;
}

.main-menu li.active a::before {
    background-color: #fff;
}

