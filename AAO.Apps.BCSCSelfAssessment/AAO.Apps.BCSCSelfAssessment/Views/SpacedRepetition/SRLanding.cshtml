@using AAO.Apps.BCSCSelfAssessment.CustomeHelper
@model  AAO.Common.BCSCSelfAssessment.ExamStatusCountSRVM
@{
    ViewBag.Title = "SRLanding";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="~/Content/SRLandingStyle.css" rel="stylesheet" />

<script src="https://www.gstatic.com/charts/loader.js"></script>
<script src="~/Scripts/Dashboard/jquery.slimscroll.js"></script>

<div class="space-page relative-position space-repit">
    <div class="col-lg-8 col-md-12 col-xs-12 xs-marbot30 marbot-30 xs-nopadding">
        <div class="widget-exams-in-progress widget-space-repetition">
            <div class="col-lg-8 col-md-12 col-sm-12 col-xs-12 nopadding  xs-textcenter  first-block">
                <h4 class="widget-title textalign">What is spaced repetition?</h4>
                <p class="textalign">
                    <span class="collapsed">
                        Spaced Repetition presents questions at random and repeats questions at spaced intervals based on the correctness of your answers and your confidence in your answer choices.
                        <a href="" onclick="javascript: void (0)">Learn more...</a>
                    </span>

                    <span class="expanded">
                        Spaced Repetition presents questions at random and repeats questions at spaced intervals based on the correctness of your answers and your confidence in your answer choices. This functionality is named for and based on the evidence-based learning technique known as spaced repetition which has been proven to increase the rate of learning and is used by software programs such as Anki, Duolingo, Picmonic and Pimsleur Language Programs. As you answer questions in this mode, choose whether you are confident in your answer (Submit Confident) or not confident in your answer (Submit Not Confident). <a href="javascript:void(0)">...Less</a>
                    </span>
                </p>
            </div>
            <div id="SpacedRepetition1">
                @using (Html.BeginForm("SpacedRepetition", "ExamManager", FormMethod.Post, new { id = "SpacedRepetition", @class = "form-inline build-exam" }))
                {
                    <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12 text-right xs-textcenter md-nopadding padtop20 txt-md-left">
                        <button class="btn btn-cta" id="SpacedRepetitionbutton">Start Spaced Repetition</button>
                    </div>

                    <hr class="visible-xs col-xs-12 nopadding" />
                    <div class="col-lg-12 col-sm-12 nopadding questionSection">
                        <div class="col-lg-3 col-md-3 col-sm-6 que-block ">
                            <div class="small">
                                <h2><span class="QuestionAnsweredSR"></span></h2>
                                <p>Questions Answered</p>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-6 que-block">
                            <div class="small">
                                <h2><span class="TopicsCoveredSR"></span></h2>
                                <p>Topics Covered</p>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-6 que-block">
                            <div class="small">
                                <h2><span class="IncorrectAnswersSR"></span></h2>
                                <p>Incorrect Answers</p>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-3 col-sm-6 que-block">
                            <div class="small">
                                <h2><span class="CorrectAnswersSR"></span></h2>
                                <p>Correct Answers</p>
                            </div>
                        </div>
                    </div>
                    <div class="clearfix"></div>

                    @*HG-459 Start - Filter to select Section*@
                    <hr class="marbot0" style=" margin-bottom: 20px;">

                    <div id="quick">
                        <div class="col-lg-12 xs-nopadding">
                            <div class="pull-left selectsection">
                                Select BCSC Section(s) <span class="font-red"> * </span>
                            </div>
                            <div class="pull-left martop2">
                                <label class="container-checkbox">
                                    <input value="" type="checkbox" id='selectall' class="TypeofCategoryList">
                                    Select all
                                    <span class='checkmark'></span>
                                </label>
                            </div>
                            <div class="pull-left col-lg-12 xs-nopadding small marl14 marbot20" id="min2secs">
                                <i>At least two (2) sections must be selected</i>
                            </div>
                            <div class="clearfix"></div>
                        </div>
                        <div class="form-group rt width100percent">
                            @Html.CheckBoxList((List<SelectListItem>)ViewBag.CategoryTypelist, "TypeofCategoryList", (String[])ViewBag.QuestionTypeListCount)
                        </div>
                        <div class="col-lg-12 text-left xs-nopadding">
                            <input type="button" value="Clear" id="Clear" class="btn btn-default" />
                        </div>
                        <div class="clearfix"></div>
                    </div>
                    @*HG-459 End*@
                }
            </div>
        </div>
    </div>

    <div class="col-lg-8 col-md-12 col-xs-12 xs-marbot30 xs-nopadding marbot-30">
        <div class="widget-exams-in-progress performance-block">
            <h4 class="widget-title">Performance Overview</h4>
            <ul class="breadcrumb-block">
                <li id="SectionButton" value="SectionButton"><span class="breadcrumb-arrow fa fa-angle-left"></span><a id="SButton" href="#!" onclick="">Sections</a></li>
                <li id="ChapterButton" value="ChapterButton"><span class="breadcrumb-arrow fa fa-angle-left"></span><a id="CButton" href="#!" onclick=" ">Chapters</a></li>
            </ul>
            <figure>
                <ul class="legend unanswer">
                    <li>Correct, Confident</li>
                    <li>Correct, Unconfident</li>
                    <li>Incorrect, Confident</li>
                    <li>Incorrect, Unconfident</li>
                    <li>Unanswered</li>
                </ul>

                <div id="graphic" class="graphic">
                    <div id="divbody">
                    </div>
                    <div id="Chapterbody">
                    </div>
                    <div id="Topicsbody">
                    </div>
                </div>
            </figure>
        </div>
    </div>

    <div class="col-lg-4 col-md-12 col-xs-12 xs-nopadding absolute-position recommended-topics-block" id="">
        <div class="widget-exam-status">
            <div class="col-lg-8 col-md-8 col-sm-8 col-xs-8 nopadding">

                <h3 class="widget-title recommended-topic" style="margin-top:4px">
                    Top Ten Recommended Topics
                </h3>
                <a data-target="#ModalCenterInfo" data-toggle="modal" href="#ModalCenterInfo">
                    <span> <img id="info" onmouseover="hover(this);" onmouseout="unhover(this);" src="\Content\img\info.svg"></span>
                </a>

            </div>
            <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4 text-right nopadding padtop20">
                <button class="btn btn-cta" value="ExportRecommendedTopics" id="ExportRecommendedTopics">Export</button>
            </div>
            <div class="clearfix"></div>
            <div class="col-lg-12 nopadding table-responsive">
                <table class="table table-striped fixed_header" id="ResidentReport" style="overflow-x:hidden; ">
                    <thead>
                        <tr>
                            <th>Section</th>
                            <th>Chapter</th>
                            <th>Topic</th>
                            <th width="50px">&nbsp;</th>
                        </tr>
                    </thead>
                    <tbody id="trbody">
                    </tbody>
                </table>
            </div>
            <div class="clearfix">
            </div>
        </div>
    </div>
</div>
