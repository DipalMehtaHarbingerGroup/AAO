                        result = $.parseJSON(result);
                        $.each(result, function (i, item) {
                            data += "<tr><td> " + item.SectionName + "</td><td>" + item.ChapterName + "</td> <td> <a href='' onclick='javascript:excerpt_popup(" + j + ");'  data-toggle='modal' data-target='#ModalCenter'>" + item.TopicName + "</a></td><td> <a href='#' ExamId=" + item.ExamId + " class='editAsset' id='delete';><i class='fa fa-trash-o text-center'></i></a></td></tr>";
                            ExcerptArray[j] = item.Excerpt;
                            j++;
