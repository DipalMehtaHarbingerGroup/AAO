            type: 'post',
            dataType: 'JSON',
            data: { ChapterNumber: ChapterValue, ChapterName: myValue, SubspecialityId: Subspeciality },
            url: '/SpacedRepetition/SRPerformanceOverviewTopics',
            success: function (response) {
                response = JSON.parse(response);
                var DivBody = document.getElementById("divbody");
