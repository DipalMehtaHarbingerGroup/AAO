        cButton.style.display = "none";
        var Chapterbody = document.getElementById("divbody");
        Chapterbody.style.display = "none";
        var Topicsbody = document.getElementById("Topicsbody");
        Topicsbody.style.display = "none";
        var DivBody = document.getElementById("Chapterbody");
        DivBody.style.display = "block";
    });
    $('#ResidentReport tbody').on('click', '#delete', function () {
        var ExamId = $(this).attr('examId');
        var x = $(this).parent('td').parent('tr').children('td')[2].innerText;
        var dataValue =
        {
            ExamId: ExamId,
            SectionName: $(this).parent('td').parent('tr').children('td')[0].innerText,
            ChapterName: $(this).parent('td').parent('tr').children('td')[1].innerText,
            TopicName: $(this).parent('td').parent('tr').children('td')[2].innerText
        }
        getConfirmation();
        function getConfirmation() {
            var retVal = $("#deleteButton").modal("show");
            $(".deleted").click(function () {
                $.ajax({
                    type: 'post',
                    dataType: 'JSON',
                    data: dataValue,
                    url: '/SpacedRepetition/DeleteTopic',
                    success: function (response) {
                        var ChapterNumber = '';
                        response = JSON.parse(response);
                    }
                });

                $.ajax({
                    type: 'Post',
                    dataType: "JSON",
                    url: '/SpacedRepetition/SRRecommendedTopics',
                    success: function (result) {
                        var data = '';
