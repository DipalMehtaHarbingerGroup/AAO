    function drawChart() {
        $.ajax({
            type: 'POST',
            dataType: "JSON",
            url: '/SpacedRepetition/SRPerformanceOverview',
            success: function (result) {
                result = $.parseJSON(result);
                $('#divbody').empty();
                $('.loader_main').fadeOut(100);

                var data = new google.visualization.DataTable();
                data.addColumn({ type: 'string', label: 'SectionTitle' });
                data.addColumn('number', 'Correct, Confident');
                data.addColumn('number', 'Correct, Unconfident');
                data.addColumn('number', 'Incorrect, Confident');
                data.addColumn('number', 'Incorrect, Unconfident');
                data.addColumn('number', 'Unanswered');
                data.addColumn({ name: 'QuestionCount', type: 'string', role: 'annotation' });

                var links = []; // Store links and IDs to be used in the event listener

                $.each(result, function (j, item) {
                    @*
                    var SubSpecialityName = "Section " + item.SectionNumber + ": " + item.SectionTitle;
                    var subspecialityId = item.SubspecialtyId; // Getting the ID for later use

                    // Construct the link using the SubspecialtyId
                    var link = 'https://www.google.com/' + subspecialityId;

                    // Store the link for later use in the event handler
                    links.push({ rowIndex: j, id: subspecialityId, link: link });
                    *@

                    // Add data to the row without using HTML inside the table
                    data.addRow([
                        item.SectionTitle, // Display plain text here
                        item.CorrectConfident,
                        item.CorrectNotconfident,
                        item.IncorrectConfident,
                        item.IncorrectNotconfident,
                        item.UnAnswered,
                        ''
                    ]);
                });

                var view = new google.visualization.DataView(data);
                view.setColumns([
                    0,
                    1, {
                        calc: function (dt, row) {
                            return dt.getValue(row, 1);
                        },
                        type: "number",
                        role: "annotation"
                    },
                    2, {
                        calc: function (dt, row) {
                            return dt.getValue(row, 2);
                        },
                        type: "number",
                        role: "annotation"
                    },
                    3, {
                        calc: function (dt, row) {
                            return dt.getValue(row, 3);
                        },
                        type: "number",
                        role: "annotation"
                    },
                    4, {
                        calc: function (dt, row) {
                            return dt.getValue(row, 4);
                        },
                        type: "number",
                        role: "annotation"
                    },
                    5, {
                        calc: function (dt, row) {
                            return dt.getValue(row, 5);
                        },
                        type: "number",
                        role: "annotation"
                    }
                ]);

                var options = {
                    animation: {
                        duration: 0,
                        easing: 'out',
                        startup: true
                    },
                    chartArea: {
                        left: 120,  // Increase space for the 1st column
                        right: 15,
                        width: '60%', // Adjust the chart's width
                        height: '85%'
                    },
                    backgroundColor: 'transparent',
                    hAxis: {
                        title: "Total Questions",
                        titleTextStyle: { italic: false },
                        //gridlines: { count: 0 }
                    },
                    colors: ['#A9C23F', '#FFDE58', '#D97B79', '#F69339', '#ddddde' @*'#919191'*@],
                    backgroundColor: { fill: '#ffffff' },
                    height: 700,
                    width: '100%',
                    legend: { position: 'none' },
                    bar: { groupWidth: '60%', groupHeight: '60%' },
                    fontSize: 12,
                    isStacked: true,
                    //tooltip: { isHtml: true },
                };

                var chart = new google.visualization.BarChart(document.getElementById('divbody'));
                chart.draw(view, options);

                // Add click event listener to handle row clicks
                @*google.visualization.events.addListener(chart, 'select', function () {
                    var selectedItem = chart.getSelection()[0];
                    if (selectedItem) {
                        var rowIndex = selectedItem.row;
                        var subspecialityId = links.find(l => l.rowIndex === rowIndex).id;
                        //alert('subspecialityId = '+subspecialityId);
                        //var link = links.find(l => l.rowIndex === rowIndex).link;
                        ChapterData(subspecialityId);
                        // Redirect to the link
                        //window.open(link, '_blank');
                    }
                });*@
            }
        });
    }

