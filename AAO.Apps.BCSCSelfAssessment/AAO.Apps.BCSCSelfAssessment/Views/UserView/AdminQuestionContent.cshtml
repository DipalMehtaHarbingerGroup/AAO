@model AAO.Common.BCSCSelfAssessment.QuestionDetails
@{
    Layout = null;
}

<div class="col-lg-12 tab-section">
    <ul class="nav nav-tabs responsive-tabs" role="tablist">
        <li role="presentation" class="active"><a href="#discussion" aria-controls="discussion" role="tab" data-toggle="tab">Discussion</a></li>
        <li role="presentation"><a href="#bcsc-excerpt" aria-controls="bcsc-excerpt" role="tab" data-toggle="tab">BCSC Excerpt</a></li>
        <li role="presentation"><a href="#references" aria-controls="references" role="tab" data-toggle="tab">References</a></li>
        <li role="presentation" id="relatedContents_tab"><a href="#relatedContents" aria-controls="relatedContents" role="tab" data-toggle="tab" id="relatedContents_tab_a">Related Content</a></li>
        <li role="presentation"><a href="#notes" aria-controls="notes" role="tab" data-toggle="tab">Notes</a></li>
    </ul>
    <div class="tab-content" id="TabContentId">
        <div role="tabpanel" class="tab-pane active padtop20" id="discussion">
            <div class="col-lg-12">

                @* <p>Proin elit arcu, rutrum commodo, vehicula tempus, commodo a, risus. Curabitur nec arcu. Donec sollicitudin mi sit amet mauris. Nam elementum quam ullamcorper ante. Etiam aliquet massa et lorem. Mauris dapibus lacus auctor risus. Aenean tempor ullamcorper leo. Vivamus sed magna quis ligula eleifend adipiscing. Duis orci. Aliquam sodales tortor vitae ipsum. Aliquam nulla. Duis aliquam molestie erat. Ut et mauris vel pede varius sollicitudin. Sed ut dolor nec orci tincidunt interdum. Phasellus ipsum. Nunc tristique tempus lectus.</p>*@
                @if (!string.IsNullOrEmpty(@Model.questionDiscussion.discussionText))
                {
                    <p>@Html.Raw(@Model.questionDiscussion.discussionText)</p>
                }
                @if (@Model.questionDiscussion.discussionImage != null)
                {
                    foreach (var image in @Model.questionDiscussion.discussionImage)
                    {
                        @*<img src="@Url.Content(image.ImagePath)" alt="Image" style="width:2%!important" />*@
                        <div class="col-lg-12 nopadding">

                            <div class="col-sm-2 col-xs-6 nopadding mar10">
                                <a href="@Url.Content(image.ImagePath)" class="thumbnail" data-toggle="modal" data-target="#myModal" data-image="@Url.Content(image.ImagePath)">
                                    <img src="@Url.Content(image.ImagePath)" id="5525" alt="Image" class="img-responsive img-center img11">
                                    <div class="zoom-icon-background text-right myDIV">
                                        <i class="glyphicon glyphicon-zoom-in text-right">
                                        </i>
                                    </div>
                                </a>
                            </div>
                        </div>
                        @*<p>&nbsp;</p>*@
                        <div class="clearfix"></div>
                    }
                }
            </div>
        </div>

        <div role="tabpanel" class="tab-pane padtop20" id="bcsc-excerpt">
            <div class="col-lg-12">
                <div class="panel-group" id="accordion">
                    @foreach (var excerptHeading in @Model.ExcerptHeadings)
                    {
                        <div class="panel panel-default notes-panel" excerptid="@(excerptHeading.HeadingId)">
                            <div class="panel-heading notes-panel-bg" role="tab">
                                <div class="panel-title view-title clearfix">
                                    <a role="button" data-toggle="collapse" class="accordian-tab notes-accordian-tab" data-parent="#accordion" href="#@(excerptHeading.HeadingId + "collapseOne")" aria-expanded="true" aria-controls="@(excerptHeading.HeadingId + "collapseOne")" excerptid="@(excerptHeading.HeadingId)">
                                        <div id="@("quesText"+excerptHeading.HeadingId)" class="question xs-padleft50 font-bold">

                                            <div id="quesTextDiv@(excerptHeading.HeadingId)">
                                                @Html.Raw(excerptHeading.HeadingText)

                                                <li class="question-number-view-exam-details_Excerpt" style="list-style-type:none;margin-top:-20px;">
                                                    <i class="fa fa-lg fa-angle-right expand"> <span class="expand">expand</span></i>
                                                </li>
                                            </div>
                                        </div>
                                    </a>
                                </div>
                            </div>
                        </div>

                        <div id="@(excerptHeading.HeadingId + "collapseOne")" class="panel-collapse collapse" aria-labelledby="@(excerptHeading.HeadingId + "headingOne")" aria-expanded="true" excerptid="@(excerptHeading.HeadingId)">
                            <div class="panel-body">
                                <div class="accordion nested-accordion" id="@(excerptHeading.HeadingId + "accordion")">
                                    <div class="accordion-group">
                                        <div class="accordion-heading">
                                            <div id="@(excerptHeading.HeadingId +"ContainerDiv")">
                                                @Html.Raw(excerptHeading.Excerpts)
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!--  End Of Accordian -->
                    }

                </div>
                <script>
                        if(@Model.ExcerptHeadings.Count<=1){
                        $('#1collapseOne').collapse({
                            toggle: true
                        });
                        $("#quesText" + 1 + " .question-number-view-exam-details_Excerpt i").removeClass("fa-angle-right").addClass("fa-angle-up");
                        }
                </script>
            </div>
        </div>
        <div role="tabpanel" class="tab-pane padtop20" id="references">
            <div class="col-lg-12">
                @foreach (var reference in @Model.references)
                {
                    <p>@Html.Raw(@reference)</p>
                }
            </div>
        </div>

        <div role="tabpanel" class="tab-pane padtop20" id="relatedContents">
            <div class="col-lg-12">
                @foreach (var relatedContent in @Model.RelatedContents)
                {
                    <p>@Html.Raw(@relatedContent)</p>
                }
            </div>
        </div>

        <div role="tabpanel" class="tab-pane padtop20" id="notes">
            <div class="col-lg-12">
                @foreach (string notes in @Model.notes)
                {
                    <p>@Html.Raw(@notes)</p>
                }

            </div>
        </div>
    </div>
</div>
<div class="clearfix">
</div>

<script>
    $(document).ready(function () {
        $('div._idGenObjectStyleOverride-1 img').addClass('thumbnail');
        $('img[class^="_idGenObjectAttribute"]').addClass('thumbnail');

        // Append zoom icon to each thumbnail in divs with id starting with "_idContainer"
        $('div._idGenObjectStyleOverride-1 .thumbnail').each(function () {
            $(this).after('<div class="zoom_icon"><i class="glyphicon glyphicon-zoom-in text-right"></i></div>');
        });

        $('img[class^="_idGenObjectAttribute"].thumbnail').each(function () {
            var zoomIconDiv = $('<div class="zoom_icon"><i class="glyphicon glyphicon-zoom-in text-right"></i></div>');
            $(this).after(zoomIconDiv);
        });

        // Event listener for all divs with id starting with "_idContainer"
        $('div._idGenObjectStyleOverride-1').on('click', function () {
            var imgElement = $(this).find('img');
            var lowResImage = imgElement.attr('src');
            var highResImage = appendSuffix(lowResImage, '_O');
            showModal(highResImage);
        });

        $('img[class^="_idGenObjectAttribute"]').on('click', function (event) {
            var lowResImage = $(this).attr('src');
            var highResImage = appendSuffix(lowResImage, '_O');
            showModal(highResImage);
            event.stopPropagation();
        });


        // Event listener for all zoom icons
        $('.zoom-icon').on('click', function (event) {
            var lowResImage = $(this).prev().attr('src');
            var highResImage = appendSuffix(lowResImage, '_O');
            showModal(highResImage);
            event.stopPropagation(); // Prevent triggering the thumbnail click event
        });

        // Function to append "_O" before the file extension
        function appendSuffix(imageSrc, suffix) {
            return imageSrc.replace(/(\.jpg|\.png|\.tif|\.tiff)$/, suffix + '$1');
        }

        // Function to show the modal with the high-resolution image
        function showModal(imageSrc) {
            $('#showimg').attr('src', imageSrc);
            $('#myModal').modal('show');
        }
    });
</script>

