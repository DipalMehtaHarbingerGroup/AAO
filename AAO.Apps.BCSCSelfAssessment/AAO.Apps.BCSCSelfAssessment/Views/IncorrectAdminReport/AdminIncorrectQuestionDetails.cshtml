    $(document).ready(function () {
        //Image Enlarge
        /****************************************/
        $(".img11").off('click');
        $(".img11").on('click', function () {
            var img = $(this).attr('src'); // get image
            $('#showimg').empty();
            $('#showimg').attr('src', img); //load image in modal
        });
        /*****************************************************/
        $(".myDIV").off('click');
